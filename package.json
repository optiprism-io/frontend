{
  "name": "optiprism",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start:dev": "vite",
    "start:preview": "vite preview",
    "generate:openapi": "openapi-generator-cli generate -i openapi.yaml -g typescript-axios --skip-validate-spec -o ./src/api",
    "clean": "rm -rf dist && mkdir ./dist",
    "fmt": "prettier ./src --write",
    "check:eslint": "eslint --ext .ts,vue ./",
    "check:vue-tsc": "vue-tsc --noEmit --skipLibCheck",
    "test:unit": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "test:e2e": "playwright test",
    "build": "vite build"
  },
  "dependencies": {
    "@antv/g2": "^4.2.7",
    "@antv/g2plot": "^2.4.20",
    "@patternfly/patternfly": "^4.183.1",
    "@vueuse/core": "^10.4.1",
    "axios": "^1.6.5",
    "floating-vue": "^2.0.0-beta.20",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.0",
    "pinia": "^2.0.22",
    "typescript-cookie": "^1.0.4",
    "valibot": "^0.25.0",
    "vue": "^3.2.39",
    "vue-grid-layout": "^2.4.0",
    "vue-router": "^4.1.5",
    "vue-virtualised": "^0.1.8",
    "vue3-drr-grid-layout": "^1.9.7",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@playwright/test": "^1.27.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.11.7",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-c8": "^0.24.3",
    "@vue/compiler-sfc": "^3.2.39",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.1",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "jsonwebtoken": "^8.5.1",
    "miragejs": "^0.1.45",
    "prettier": "^3.2.2",
    "sass": "^1.54.9",
    "typescript": "~5.3.3",
    "vite": "^5.0.3",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.8.27"
  }
}
